- @latest_members.each do |latest_member|
  %li.span1
    %a.thumbnail{"data-original-title" => "#{latest_member.profile.name if !latest_member.profile.blank?}", "data-placement" => "bottom", "href" => "/profile/#{latest_member.permalink}", "rel" => "tooltip"}
      %img{:src => "https://encrypted-tbn1.google.com/images?q=tbn:ANd9GcSYhnBvyrRrhumLTwSW7CLDFLVNY_ONEAHBFMdxc46bJLU8nuGLZw"}

:javascript
  $(function () {
    $('a[rel=tooltip]').tooltip({
        live: true
    })
  });